{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "build:legacy": {
      "dependsOn": ["^build:legacy"],
      "outputs": ["dist/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "turbo:dev": {
      "cache": false,
      "persistent": true
    },
    "client:dev": {
      "cache": false,
      "persistent": true
    },
    "server:dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {
      "outputs": []
    },
    "lint:legacy": {
      "outputs": []
    },
    "preview": {
      "cache": false,
      "persistent": true
    }
  }
}